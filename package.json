  "private": true,
  "scripts": {
    "debug:web": "node config/start.js",
    "debug:server": "nodemon server/debugServer.js",
    "debug:web:dev": "cross-env REACT_APP_ENV=dev npm run debug:web",
    "debug:web:test": "cross-env REACT_APP_ENV=test npm run debug:web",
    "debug:web:move": "cross-env REACT_APP_ENV=move npm run debug:web",
    "clear": "rimraf build/static",
    "web:build": "node config/webBuild.js",
    "server:build": "node config/serverBuild.js",
    "build": "npm run web:build && npm run server:build",
    "build:dev": "npm run clear && cross-env REACT_APP_ENV=dev& cross-env NODE_ENV=production npm run build",
    "build:test": "npm run clear && cross-env REACT_APP_ENV=test npm run build",
    "build:move": "npm run clear && cross-env REACT_APP_ENV=move npm run build",
    "build:prod": "npm run clear && cross-env REACT_APP_ENV=prod npm run build",
    "server:start:dev": "pm2 startOrReload ecosystem.config.js --env dev",
    "server:start:test": "pm2 startOrReload ecosystem.config.js --env test",
    "server:start:move": "pm2 startOrReload ecosystem.config.js --env move",
    "server:start:prod": "pm2 startOrReload ecosystem.config.js --env prod",
    "server:stop": "pm2 stop ecosystem.config.js"
  },
